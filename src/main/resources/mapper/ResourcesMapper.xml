<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rui.myblog.mapper.ResourcesMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rui.myblog.pojo.Resources">
        <id column="id" property="id" />
        <result column="resource_name" property="resourceName" />
        <result column="resource_address" property="resourceAddress" />
        <result column="first_type" property="firstType" />
        <result column="second_type" property="secondType" />
        <result column="picture_address" property="pictureAddress" />
        <result column="resource_description" property="resourceDescription" />
        <result column="create_time" property="createTime" />
        <result column="sort" property="sort" />
        <result column="published" property="published" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, resource_name, resource_address, first_type, second_type, picture_address, resource_description, create_time, sort, published
    </sql>

</mapper>
